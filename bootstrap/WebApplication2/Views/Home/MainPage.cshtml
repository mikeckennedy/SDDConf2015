
@{
    ViewBag.Title = "MainPage";
}


<div>
	<h1>This is our slogan</h1>
	<img
		class="hidden-xs"
		 src="http://cdn.youthnet.org/uploads/2014/07/rants-banner.jpg" />
</div>

<div class="container">
	<div class="row">

		<div class="col-sm-4"
			 style="background: yellow">
			<br/>
			<br/>
			<br/>
		</div>
		<div class="col-sm-8"
		     style="background: red">
			<br />
			<br />
			<br />
		</div>

	</div>
</div>



<table class="table table-hover table-striped">
	<thead>

	<tr>
		<th>Name</th>
		<th>Address</th>
		<th>Count</th>

	</tr>

	</thead>
	<tbody>

	@for (int i = 0; i < 10; i++)
	{
		<tr>
			<td>Name @i</td>
			<td>Address @i</td>
			<td>Count @i</td>
		</tr>
	}

	</tbody>
</table>

<style>
	.social {
		font-size: 96px;
	}
	.social > a {
		color: gray;
	}
	.social > a:hover {
		color: red;
	}
</style>

<div class="social">

	<a href="http://twitter.com/mkennedy"><i class="fa fa-twitter"></i></a> |
	<a href="http://youtube.com"><i class="fa fa-youtube-square"></i></a> |
	<a href="http://soundcloud.com"><i class="fa fa-soundcloud"></i></a> 

</div>